{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": false,
      "dynamicImport": true
    },
    "paths": {
      "@shared": ["../shared/*"]
    },
    "transform": {
      "react": {
        "pragma": "h",
        "pragmaFrag": "Fragment",
        "refresh": true
      }
    },
    "target": "es2015"
  },
  "minify": true
}
